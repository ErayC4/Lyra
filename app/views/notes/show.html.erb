.dropdown-container {
  position: relative;
  display: inline-block;
}

.dropdown-menu {
  position: absolute;
  top: 100%;
  right: 0;
  background-color: white;
  border: 1px solid #ccc;
  box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.1);
  display: none;
  flex-direction: column;
  min-width: 150px;
  z-index: 1000;
}

.dropdown-menu a {
  padding: 10px;
  display: block;
  text-decoration: none;
  color: black;
}

.dropdown-menu a:hover {
  background-color: #f0f0f0;
}

.dropdown-button {
    position: relative;
  }
  
  /* Dropdown content (hidden by default) */
  .dropdown-content {
    display: none;
    position: absolute;
    right: 40px; /* Position to the right of the button */
    top: 0;
    background-color: white;
    min-width: 250px;
    width: 300px;
    border: 1px solid #DBDBDB;
    border-radius: 8px;
    box-shadow: 0px 2px 10px rgba(0,0,0,0.1);
    z-index: 1000;
    max-height: 500px;
    overflow: hidden;
    display: flex;
    flex-direction: column;
  }
  
  /* Show the dropdown menu when button is clicked */
  .dropdown-button:focus + .dropdown-content,
  .dropdown-content:hover {
    display: flex;
  }
  
  /* Dropdown header with search */
  .dropdown-header {
    padding: 10px;
    border-bottom: 1px solid #DBDBDB;
  }
  
  .search-container {
    display: flex;
    flex-direction: column;
    gap: 5px;
  }
  
  .archived-label {
    font-weight: bold;
    font-size: 14px;
  }
  
  .search-box {
    display: flex;
    align-items: center;
    background-color: #f0f0f0;
    border-radius: 5px;
    padding: 5px 8px;
  }
  
  .search-box input {
    border: none;
    background: transparent;
    margin-left: 5px;
    width: 100%;
    outline: none;
    font-size: 14px;
  }
  
  /* Dropdown body with sections */
  .dropdown-content-body {
    overflow-y: auto;
    flex-grow: 1;
  }
  
  .section {
    margin-bottom: 10px;
  }
  
  .section-header {
    padding: 5px 10px;
    font-size: 12px;
    color: #666;
  }
  
  .note-item {
    padding: 10px;
    display: flex;
    justify-content: space-between;
    border-left: 3px solid transparent;
  }
  
  .note-item.active {
    background-color: #e6ffe6;
    border-left: 3px solid #00cc00;
  }
  
  .note-text p {
    margin: 0;
    font-size: 14px;
    line-height: 1.2;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
  }
  
  .note-delete {
    opacity: 0;
    transition: opacity 0.2s;
  }
  
  .note-item:hover .note-delete {
    opacity: 1;
  }
  
  /* Dropdown footer */
  .dropdown-footer {
    padding: 10px;
    border-top: 1px solid #DBDBDB;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 12px;
    color: #666;
  }
  
  .scroll-up {
    cursor: pointer;
  }
  
  /* Animation for opening/closing */
  @keyframes slideRight {
    from {transform: translateX(-20px); opacity: 0;}
    to {transform: translateX(0); opacity: 1;}
  }
  
  @keyframes slideLeft {
    from {transform: translateX(0); opacity: 1;}
    to {transform: translateX(-20px); opacity: 0;}
  }
  
  .dropdown-content {
    animation: slideRight 0.2s ease;
  }
  
  .dropdown-button:not(:focus) + .dropdown-content:not(:hover) {
    animation: slideLeft 0.2s ease forwards;
  }

